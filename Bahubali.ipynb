   "source": [
    "a = input(\"Enter your number: \")\n",
    "b = len(a)\n",
    "l=[]\n",
    "for i in range(0, b):\n",
    "    c = a[i]\n",
    "    d = int(c)**b\n",
    "    l.append(d)\n",
    "if sum(l) == int(a):\n",
    "    print(\"The number is armstrong!\")\n",
    "else:\n",
    "    print(\"It isn't!\")\n",
    "#use 9474 and 1234 for testing"
   ]
